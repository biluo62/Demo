    "postcss-import": "12.0.1",
    "stylelint": "9.8.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "1.0.0"
  },
  "engines": {
    "node": "8.6.0"
  },
  "dependencies": {
    "antd": "3.4.1"
  },
  "browserslist": [
    "> 0.04%",
    "last 5 versions",
    "not ie < 8"
